{
  "name": "nest",
  "version": "1.0.0",
  "description": "",
  "author": "Oleh Chaplia",
  "license": "Proprietary",
  "scripts": {
    "format": "prettier --write \"**/*.js\"",
    "start": "babel-node index.js",
    "start:dev": "nodemon",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev": "nodemon index.js",
    "lint": "node ./node_modules/eslint/bin/eslint ./app/**/*.js --color",
    "prettier-check": "npx prettier --check ./app",
    "prettier-write": "npx prettier --write ./app",
    "circular": "madge --circular ./app",
    "dep": "npx depcruise src --output-type dot | dot -T svg > ./assets/depgraph.svg",
    "depng": "npx depcruise src --output-type dot | dot -Gdpi=300 -T png > ./assets/depgraph.png",
    "loc": "npx cloc app"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/register": "^7.18.6",
    "@babel/runtime": "^7.18.6",
    "@nestjs/testing": "^10.0.0",
    "code-complexity": "^4.4.4",
    "dependency-cruiser": "^16.2.0",
    "jest": "^28.1.2",
    "madge": "^6.1.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "rootDir": "src",
    "testRegex": ".spec.js$",
    "coverageDirectory": "../coverage"
  }
}
